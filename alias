alias rungradle='./gradlew run -DUseMemcached=true'
alias status='git status'
alias gfh='git log --oneline -n 10 --topo-order --graph'
alias gfl='git log --color --oneline --topo-order --graph --branches | less -SR'
alias gco='git checkout'
alias gb='git branch'
alias vim='nvim'
alias vi='nvim'
alias cdsrc='cd site-server/src/main/webapp/universal/src/'
alias vid='nvim `git diff --name-only master...HEAD`'
alias vimrc='nvim ~/.config/nvim/init.vim'
